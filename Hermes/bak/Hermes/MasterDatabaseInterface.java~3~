public class MasterDatabaseInterface{

  public static void addCarrier(){
    DatabaseInterface database = new DatabaseInterface(DatabaseInterface.REGISTERSHIPCAR);
    database.queryDatabase("");
  }

  public static void addShipper(){
    DatabaseInterface database = new DatabaseInterface(DatabaseInterface.REGISTERSHIPCAR);
    database.queryDatabase("");
  }

  public static void removeCarrier(){
    DatabaseInterface database = new DatabaseInterface(DatabaseInterface.REGISTERSHIPCAR);
    database.queryDatabase("");
  }

  public static void removeShipper(){
    DatabaseInterface database = new DatabaseInterface(DatabaseInterface.REGISTERSHIPCAR);
    database.queryDatabase("");
  }
}